grammar TaskTime
  rule main
    name time_setting:time?
  end

  rule name
    ([^ ] / ' ' !time)+
  end
  
  rule time
    ' '? (inc_dec / range)
  end
   
  rule inc_dec
    [+-] [0-9]+ [smhd]
  end
  
  rule range
    hour minute? ('-' range)?
  end
  
  rule hour
    [0-1] [0-9] / '2' [0-4]
  end
  
  rule minute
    [0-5] [0-9] / '6' '0'
  end
end